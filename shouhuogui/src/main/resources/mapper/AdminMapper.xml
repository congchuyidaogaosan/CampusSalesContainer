<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.AdminMapper">

    <resultMap id="BaseResultMap" type="com.domain.Admin">
            <id property="adminId" column="admin_id" jdbcType="INTEGER"/>
            <result property="adminName" column="admin_name" jdbcType="VARCHAR"/>
            <result property="adminAccount" column="admin_account" jdbcType="VARCHAR"/>
            <result property="adminPassword" column="admin_password" jdbcType="VARCHAR"/>
            <result property="adminRole" column="admin_role" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        admin_id,admin_name,admin_account,
        admin_password,admin_role
    </sql>
</mapper>
